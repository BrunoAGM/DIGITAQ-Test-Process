      "type" : "url",
      "value" : [
        "../{{context.students_ref.link}}"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "currentTeacher" : {
      "type" : "url",
      "value" : [
        "../API/system/session/1"
      ],
      "exposed" : false
    },
    "teacher_query" : {
      "type" : "businessdata",
      "value" : [
        "{\"displayValue\":\"find [com.company.model.Teacher]\",\"businessObjectName\":\"Teacher\",\"query\":{\"name\":\"find\",\"displayName\":\"find\"},\"id\":\"com.company.model.Teacher\",\"filters\":[],\"pagination\":{\"p\":\"0\",\"c\":\"10\"}}"
      ],
      "exposed" : false
    },
    "Groups" : {
      "type" : "expression",
      "value" : [
        "var teacher = $data.teacher_query.find(it => it.userName === $data.currentTeacher.user_name);",
        "return teacher.groups;"
      ],
      "exposed" : false
    },
    "formInput" : {
      "type" : "json",
      "value" : [
        "[]"
      ],
      "exposed" : false
    },
    "setInput" : {
      "type" : "expression",
      "value" : [
        "",
        "$data.Groups.forEach(item1=>{",
        "    item1.students.forEach(item2=>{",
        "      var student ={",
        "          \"persistenceId_string\":item2.persistenceId_string,",
        "          \"mark\":item2.mark",
        "      };",
        "      var result =  $data.formInput.find(it => it.persistenceId_string === student.persistenceId_string);",
        "      if(!result){",
        "             $data.formInput.push(student);",
        "      }",
        "    });  ",
        "});"
      ],
      "exposed" : false
    }
