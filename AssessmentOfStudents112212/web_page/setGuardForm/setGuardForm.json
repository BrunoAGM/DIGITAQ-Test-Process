        "$data.teachers.forEach(item=>{",
        "  var teacher={",
        "      \"name\":item.firstname+\" \"+item.lastname,",
        "      \"id\":item.id",
        "  };",
        "  teachersNames.push(teacher);",
        "});",
        "",
        "return teachersNames;"
      ],
      "exposed" : false
    },
    "initExamContract" : {
      "type" : "expression",
      "value" : [
        "",
        "",
        "  $data.schedule.forEach(item => {",
        "    const examVar = {",
        "      \"module\": item.moduleExam,",
        "      \"guards\": [],",
        "      \"persistenceId_string\":\"\"",
        "    };",
        "    const examContracts = $data.formInput.examContract;",
        "    const existingExam = examContracts.find(exam => exam.module === examVar.module);",
        "    if (!existingExam) {",
        "      examContracts.push(examVar);",
        "    }",
        "  });"
      ],
      "exposed" : false
    },
    "exam" : {
      "type" : "json",
      "value" : [
