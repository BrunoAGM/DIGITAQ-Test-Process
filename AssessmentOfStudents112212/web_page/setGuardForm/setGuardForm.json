      "type" : "expression",
      "value" : [
        "const examContracts = $data.formInput.examContract;",
        "const existingExam = examContracts.find(exam => exam.module === $data.selectedExam.moduleExam);",
        "$data.exam = existingExam;"
      ],
      "exposed" : false
